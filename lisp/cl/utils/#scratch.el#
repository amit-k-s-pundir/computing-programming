(defun copy-fn-to-file (file)
  (save-excursion 
    (let* ((beg (progn(beginning-of-defun)
		    (point)))
	  (end (progn (end-of-defun)
		    (point)))
	  (reg (copy-region-as-kill beg end)))
      (goto-line (+ 1 end))
      (yank))))
