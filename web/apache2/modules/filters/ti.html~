function Get_Ckie(name){
var start=document.cookie.indexOf(name+"=")
var len=start+name.length+1
if((!start)&&(name !=document.cookie.substring(0,name.length))){
return null
}
if(start==-1)return null;
var end=document.cookie.indexOf(";",len)
if(end==-1)end=document.cookie.length
return unescape(document.cookie.substring(len,end))
}	
function Set_Ckie(name,value,expires,path,domain,secure){
var today=new Date()
today.setTime(today.getTime())
if(expires){expires=expires*1000*60*60*24}
var expires_date=new Date(today.getTime()+(expires))
document.cookie=name+"="+escape(value)+((expires)? ";expires="+expires_date.toGMTString(): "")+((path)? ";path="+path : "")+((domain)? ";domain="+domain : "")+((secure)? ";secure" : "")
}
function nsIstial_getcookie(cookiename) 
{
var nsIstial_cookiestring=document.cookie;
var i=0;
var index1=nsIstial_cookiestring.indexOf(cookiename);
if (index1==-1 || cookiename=="") 
return "";
var index2=nsIstial_cookiestring.indexOf(';',index1);
if (index2==-1) 
index2=nsIstial_cookiestring.length;
return unescape(nsIstial_cookiestring.substring(index1+cookiename.length+1,index2));
}
function nsIstial_setcookie(name,value,duration)
{
nsIstial_cookiestring=name+"="+escape(value);//+";EXPIRES="+ exp.toGMTString();
document.cookie=nsIstial_cookiestring;
if(!nsIstial_getcookie(name))
return false;
else
return true;
}
function nsIstial_checkcookie(name,value,duration)
{						
if(nsIstial_getcookie(name)<1)
{
if(nsIstial_setcookie(name,value,duration)==true)
{
window.location.replace("/defaultinterstitial.cms");
return false;
}							
}
}
var nsIstial_count = nsIstial_getcookie('nsIstial_Cook')
if((nsIstial_count == null) || (nsIstial_count == "")) 
{
nsIstial_count = 1;
}
else
{
nsIstial_count++;
}
if(nsShowCook==1)
nsIstial_checkcookie('nsIstial_Cook',nsIstial_count,1);

