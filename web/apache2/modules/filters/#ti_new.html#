				  rv = frame.contentDocument;
				  else // bad IE  ;)
				  rv = document.frames[aID].document;												
				  return rv;
				  }
				  function adjustMyFrameHeight(){																	
				  var frame = getElement("mailroifrm12");											
				  var frameDoc = getIFrmDoc("mailroifrm12");											
				  frame.height = frameDoc.body.scrollHeight;											
				  }
				  setTimeout("adjustMyFrameHeight();",2000)
				</script>
