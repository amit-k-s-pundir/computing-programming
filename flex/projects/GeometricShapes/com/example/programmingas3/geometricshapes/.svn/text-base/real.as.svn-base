package com.example.programmingas3.geometricshapes
  {
	function getArea(arFn:Function, poly:IPolygon):Number{
//	  poly.getArea();
	  arFn(poly);
	}

	function getPerimeter(poly:IPolygon):Number{
	  poly.getPerimeter();
	}

	function getSumOfAngles(poly:IPolygon):Number{
	  poly.getSumOfAngles();
	}

	function describe(poly:IPolygon):String{
	  poly.describe();
	}
  }
