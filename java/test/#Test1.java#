	public interface IAddn{
		public int addn(int x);
	}
	public IAddn getAddn(final int p){
	return	new IAddn(){
			public int addn(int x){
				return (p + x);
			};
		};
	}
        public static void main(String[] args) {
		System.out.println((new Test1()).getAddn(5).addn(2));
